<Window x:Class="My_Neighborhood_WPF_.PS4ConsolePages.PrefSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:My_Neighborhood_WPF_.PS4ConsolePages"
        mc:Ignorable="d"
        Title="PrefSettings" Height="300" Width="300" Icon="/My Neighborhood(WPF);component/Images/favicon (1).ico" Loaded="Window_Loaded">
    <Grid>
        <GroupBox x:Name="groupBox" Header="Preference settings" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="144" Width="274">
            <CheckBox x:Name="chb" Content="Enable UART" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Checked="chb_Checked" Unchecked="chb_Unchecked"/>

        </GroupBox>

    </Grid>
</Window>
